{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/projects", "dest": "server.js" },
      { "src": "/api/projects/:id", "dest": "server.js" },
      { "src": "/api/data", "dest": "server.js" },
      { "src": "/(.*)", "dest": "server.js" }
    ]
  }
  